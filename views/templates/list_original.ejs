<div class="CourseCard-StyledCourseCard">

    
    <div class="Coursecard-InfoRatingWrapper">

        <div class="CourseCard-NumRatingWrapper-syjs0d-2 joEEbw">
            <div class="CardNumRating-StyledCardNumRating">

                <% const randomNumber = Math.floor(Math.random() * 9) + 1; %>
               
                 <div class="CardNumRating-CardNumRatingNumber">
              
                    <img src="/image/randomimage/<%= randomNumber %>.jpeg" style='width:200px; height:200px'alt="Image Description">
                    
                 </div>
          
            </div>
        </div>
    
        <div class="CourseCard_CardInfo">
            <div class="Card-CourseName"><%= result.Title %></div>
            <div class="Card-CourseProvider"><b>Provider:</b> <%= result.Provider %></div>
            <div class="Card-CourseRatingCoursla"><b>Coursla Rating:</b> </div>
            <div class="Card-CourseRating"><b>Provider Rating:</b> <%= result.Course_Rating %></div>
            <div class="Card-CourseDifficulty"><b>Level:</b> <%= result.Course_Difficulty %></div>
            
        </div>
    </div>


    <% if (isLoggedIn) { %>
        <button class="Course-Bookmark <%= userBookmarks && userBookmarks.includes(result._id.toString()) ? 'bookmarked' : '' %>" type="button" data-course-id="<%= result._id %>" onclick="toggleBookmark('<%= result._id %>')">    
    <% } else { %>
        <button class="Course-Bookmark" type="button" data-course-id="<%= result._id %>" onclick="toggleBookmark('<%= result._id %>')">
    <% } %>
