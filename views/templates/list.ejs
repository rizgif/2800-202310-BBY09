<script>
  function handleClick(courseid) {
    console.log('Clicked on', courseid);

    // Send a POST request to the server route
    $.get('/course-details', function(response) {
      console.log('Redirecting to new page');
      window.location.href = '/course-details?courseid=' + courseid;
    });
  }
</script>
   
